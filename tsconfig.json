{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"verbatimModuleSyntax": true,
		"sourceMap": true,
		"strict": true,
		"lib": ["esnext", "dom"],
		"types": ["svelte", "vite/client"],
		"moduleResolution": "bundler"
	}
}
