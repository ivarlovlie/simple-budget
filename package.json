{
	"name": "budsjett",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"db:generate": "drizzle-kit generate:pg",
		"db:push": "drizzle-kit push:pg",
		"db:check": "drizzle-kit check:pg",
		"db:drop": "bunx drizzle-kit drop",
		"test": "bun run test:integration && bun run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.1",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.16.0",
		"@typescript-eslint/parser": "^7.16.0",
		"autoprefixer": "^10.4.19",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.20.18",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"postcss": "^8.4.39",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.3",
		"vitest": "^1.6.0",
		"@inlang/paraglide-js": "1.11.2"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/svelte-table": "^8.19.3",
		"bits-ui": "^0.21.12",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.30.10",
		"drizzle-zod": "^0.5.1",
		"formsnap": "^1.0.1",
		"lucia": "^3.2.0",
		"mode-watcher": "^0.3.1",
		"ofetch": "^1.3.4",
		"oslo": "^1.2.1",
		"paneforge": "^0.0.4",
		"postgres": "^3.4.4",
		"scheduler-polyfill": "^1.2.1",
		"svelte-interactions": "^0.2.0",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.27",
		"sveltekit-superforms": "^2.16.0",
		"temporal-polyfill": "^0.2.5",
		"zeed": "^0.20.10",
		"@inlang/paraglide-sveltekit": "0.11.0"
	}
}
